<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">

    <title>Mego | Administration</title>
    
    <link rel="icon" sizes="600x600" href="/img/logo.png">
    
    <link rel="stylesheet" type="text/css" href="/css/googlefonts.css">
    <link rel="stylesheet" href="/vendor/boostrap/css/bootstrap.min.css">
    <link rel="stylesheet" href="/vendor/uniform/css/uniform.default.min.css">
    <link rel="stylesheet" href="/vendor/font-awsome/css/font-awesome.min.css">
    <link rel="stylesheet" href="/vendor/simple-line-icons/simple-line-icons.min.css">
    <link rel="stylesheet" type="text/css" href="/vendor/datatables/plugins/bootstrap/dataTables.bootstrap.css">

    <link rel="stylesheet" href="/css/components.css">
    <link rel="stylesheet" href="/css/plugins.css">
    <link rel="stylesheet" href="/theme/css/layout.css">
    <link rel="stylesheet" href="/theme/css/darkblue.css">

    <style type="text/css">
        table tbody {
            font-size: 13px !important;
        }
    </style>
</head>
<body class="page-header-fixed page-quick-sidebar-over-content page-sidebar-closed-hide-logo">
    <!-- begin header -->
    <% include ../partials/header %>
    <!-- end header -->

    <div class="clearfix"></div>

    <!-- begin container -->
    <div class="page-container">
        <!-- begine sidebar -->
        <% var activeItem= 'attendance_records'; %>
        <% include ../partials/sidebar %>
        <!-- end sidebar -->

        <!-- begin content -->
        <div class="page-content-wrapper">
            <div class="page-content" style="min-height:900px;">
                <!-- begin page header -->
                <div class="page-bar">
                    <ul class="page-breadcrumb">
                        <li>
                            <i class="icon-notebook"></i>
                            <a href="javascript:;">Attendance Records</a>
                        </li>
                    </ul>
                </div>
                <!-- end page header --> 

                <!-- begin page content -->
                <div class="row">
                    <div class="col-md-12">
                        <!-- begine table content -->
                        <table class="table table-striped table-bordered table-hover" role="grid" id="attendance_grid">
                        <!-- begin grid header -->
                        <thead>
                            <tr role="row">
                                <th tabindex="0" rowspan="1" colspan="1">Room</th>
                                <th tabindex="1" rowspan="1" colspan="1">Meeting Name</th>
                                <th tabindex="2" rowspan="1" colspan="1">Employee</th>
                                <th tabindex="3" rowspan="1" colspan="1">Attendance Time</th>
                            </tr>
                        </thead>
                        <!-- end grid header -->
                        </table>
                        <!-- end table content -->
                    </div>
                </div>
                <!-- end page content -->
            </div>
        </div>
        <!-- end content -->

        <!-- begin footer -->
        <% include ../partials/footer %>
        <!-- end footer -->
    </div>
    <!-- end container -->

    <!-- begin javascript -->
    <!-- begin core plugins -->
    <script src="/vendor/jquery.min.js" type="text/javascript"></script>
    <script src="/vendor/boostrap/js/bootstrap.min.js" type="text/javascript"></script>
    <script type="text/javascript" src="/vendor/datatables/media/js/jquery.dataTables.min.js"></script>
    <script type="text/javascript" src="/vendor/datatables/plugins/bootstrap/dataTables.bootstrap.js"></script>
    <script src="/theme/scripts/metronic.js" type="text/javascript"></script>
    <script src="/theme/scripts/layout.js" type="text/javascript"></script>
    <!-- end core plugins -->
    <script type="text/javascript">
        jQuery(document).ready(function() {
            Metronic.init();
            Layout.init();
        });
    </script>
    <script src="/scripts/admin/attendance.js" type="text/javascript"></script>
    <!-- end javascript -->
</body>
</html>